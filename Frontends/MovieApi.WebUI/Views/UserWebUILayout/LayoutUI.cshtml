
@{
    Layout = null;
}

<!DOCTYPE html>
<!--[if IE 7]>
<html class="ie ie7 no-js" lang="en-US">
<![endif]-->
<!--[if IE 8]>
<html class="ie ie8 no-js" lang="en-US">
<![endif]-->
<!--[if !(IE 7) | !(IE 8)  ]><!-->
<html lang="en" class="no-js">

<!-- movielist07:38-->
@await Component.InvokeAsync("_UserLayoutWebUIHeadComponentPartial")
<body>
    <!--preloading-->
    @await Component.InvokeAsync("_UserLayoutWebUIPreloaderComponentPartial")
    <!--end of preloading-->
    <!--login form popup-->
    @await Component.InvokeAsync("_UserLayoutWebUILoginModalComponentPartial")
    <!--end of login form popup-->
    <!--signup form popup-->
    @await Component.InvokeAsync("_UserLayoutWebUIRegisterModalComponentPartial")
    <!--end of signup form popup-->
    <!-- BEGIN | Header -->
    @await Component.InvokeAsync("_UserLayoutWebUINavbarComponentPartial")
    <!-- END | Header -->
    @await Component.InvokeAsync("_UserLayoutWebUIHeroComponentPartial")

    @RenderBody()
    <!-- footer section-->
    @await Component.InvokeAsync("_UserLayoutWebUIFooterComponentPartial")
    <!-- end of footer section-->
    <!-- scripts -->
    @await Component.InvokeAsync("_UserLayoutWebUIScriptComponentPartial")
    <div id="loginSpinner" class="text-center d-none">
        <div class="spinner-border text-primary" role="status">
            <span class="visually-hidden">Yükleniyor...</span>
        </div>
        <p class="mt-2">Giriş yapılıyor...</p>
    </div>
    <!--end of scripts -->
    @RenderSection("Scripts", false)
</body>
<!-- movielist07:38-->
</html>
